{
    "name": "nthmedia/photo-exif",
    "description": "The plugin reads EXIF data from photos",
    "type": "craft-plugin",
    "version": "2.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "photo exif"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "NTH media",
            "homepage": "https://nthmedia.nl"
        }
    ],
    "require": {
        "php": "^8.0",
        "craftcms/cms": "^4.0"
    },
    "autoload": {
        "psr-4": {
          "nthmedia\\photoexif\\": "src/"
        }
    },
    "extra": {
        "name": "Photo EXIF",
        "handle": "photo-exif",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "???",
        "components": {
            "metadata": "nthmedia\\photoexif\\services\\Metadata"
        },
        "class": "nthmedia\\photoexif\\PhotoExif"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "craftcms/rector": "dev-main",
        "craftcms/ecs": "dev-main",
        "phpstan/phpstan": "^1.6"
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}
